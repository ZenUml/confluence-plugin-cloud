{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/atlassian-connect*.json",
        "function": "descriptor"
      },
      {
        "source": "/*/attachment",
        "function": "renderAttachment"
      },
      {
        "source": "/*/installed",
        "function": "installedEndpoint"
      },
      {
        "source": "/*/uninstalled",
        "function": "uninstalledEndpoint"
      }
    ]
  }
}
