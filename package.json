{
  "name": "confluence-plugin",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "engine": "use yarn to install",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "build:full": "PRODUCT_TYPE=full vue-cli-service build --mode production",
    "build:full:watch": "PRODUCT_TYPE=full vue-cli-service build --watch",
    "build:lite": "PRODUCT_TYPE=lite vue-cli-service build",
    "wrangler:serve": "wrangler pages dev dist",
    "lint:vue": "vue-cli-service lint",
    "git:branch:clean:gone": "git fetch -p && git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -d",
    "git:branch:clean:merged": "git branch --merged|egrep -v \"(\\*|master|main|dev|skip_branch_name)\" | xargs git branch -d",
    "start:local": "VERSION=latest PRODUCT_TYPE=lite vue-cli-service serve",
    "start:sit": "concurrently \"yarn wrangler:serve\" \"yarn start:local\" ",
    "install:stg": "pluploader --base-url https://zenuml-stg.atlassian.net/wiki install --cloud --plugin-uri https://air.zenuml.com/descriptor"
  },
  "dependencies": {
    "html-to-image": "^1.9.0",
    "lodash": "^4.17.21",
    "lzutf8": "^0.5.6",
    "md5": "^2.3.0",
    "mermaid": "^9.1.6",
    "split.js": "^1.6.5",
    "swagger-editor": "^4.4.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "vue": "^2.6.12",
    "vue-codemirror": "^4.0.6",
    "vue-gtag": "^1.16.1",
    "vue-sequence": "^1.0.237",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.13.5",
    "@cloudflare/workers-types": "^3.0.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/jest": "^24.0.19",
    "@types/mermaid": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/cli": "^5.0.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "@vue/vue2-jest": "^28.0.1",
    "autoprefixer": "^9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^6.2.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^27.1.0",
    "longjohn": "^0.2.12",
    "postcss": "^7",
    "speed-measure-webpack-plugin": "^1.5.0",
    "test-lib": "^2.1.2",
    "toucan-js": "^2.6.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.7.4",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.12",
    "wrangler": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 years"
  ],
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "engines": {
    "node": ">=14"
  }
}
