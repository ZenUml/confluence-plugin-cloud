{
  "key": "com.zenuml.confluence-addon-lite",
  "name": "ZenUML Lite",
  "description": "ZenUML Lite add-on",
  "baseUrl": "{{localBaseUrl}}",
  "authentication": {
    "type": "none"
  },
  "lifecycle": {
    "installed": "/installed?version=2021.4",
    "uninstalled": "/uninstalled?version=2021.4"
  },
  "enableLicensing": false,
  "links": {
    "self": "/atlassian-connect-lite.json",
    "homepage": "https://www.zenuml.com"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "zenuml-sequence-macro-lite",
        "name": {
          "value": "ZenUML Sequence Lite"
        },
        "renderModes": {
          "default": {
            "url": "/attachment?version=2021.11&spaceKey={space.key}&pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence"
          }
        },
        "url": "/sequence-viewer.html?version=2021.11&spaceKey={space.key}&pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}&outputType={output.type}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence",
        "description": {
          "value": "Create a ZenUML Sequence Diagram"
        },
        "outputType": "block",
        "bodyType": "plain-text",
        "editor": {
          "url": "/sequence-editor.html?version=2021.11&update=202103291207&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence",
          "width": "100%",
          "height": "100%",
          "cacheable": true
        },
        "icon": {
          "width": 16,
          "height": 16,
          "url": "image/zenuml_logo.png"
        },
        "documentation": {
          "url": "https://zenuml.atlassian.net/wiki/spaces/Doc/overview"
        },
        "categories": [
          "visuals"
        ],
        "featured": true,
        "parameters": [
          {
            "identifier": "diagramName",
            "name": {
              "value": "Diagram name (do not change)",
              "i18n": "diagramName"
            },
            "type": "string",
            "required": true,
            "multiple": false,
            "defaultValue": "None"
          }
        ]
      },
      {
        "key": "zenuml-graph-macro-lite",
        "name": {
          "value": "ZenUML Graph Lite"
        },
        "renderModes": {
          "default": {
            "url": "/attachment?version=2021.11&spaceKey={space.key}&pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-graph"
          }
        },
        "url": "/drawio/viewer.html?version=2021.11&spaceKey={space.key}&pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}&outputType={output.type}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-graph",
        "description": {
          "value": "Create a ZenUML Graph"
        },
        "outputType": "block",
        "bodyType": "none",
        "editor": {
          "url": "/drawio/editor.html?version=2021.11&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-graph",
          "editTitle": {
            "value": "Edit ZenUML Graph"
          },
          "insertTitle": {
            "value": "Create ZenUML Graph"
          },
          "width": "100%",
          "height": "100%",
          "cacheable": true
        },
        "icon": {
          "width": 16,
          "height": 16,
          "url": "image/zenuml_logo.png"
        },
        "documentation": {
          "url": "https://zenuml.atlassian.net/wiki/spaces/Doc/overview"
        },
        "categories": [
          "visuals"
        ],
        "featured": true,
        "parameters": [
          {
            "identifier": "diagramName",
            "name": {
              "value": "Diagram name (do not change)",
              "i18n": "diagramName"
            },
            "type": "string",
            "required": true,
            "multiple": false,
            "defaultValue": "None"
          }
        ]
      }
    ],
    "customContent": [
      {
        "uiSupport": {
          "contentViewComponent": {
            "moduleKey": "zenuml-content-sequence-viewer"
          },
          "icons": {
            "item": {
              "width": 16,
              "height": 16,
              "url": "image/zenuml_logo.png"
            }
          }
        },
        "apiSupport": {
          "bodyType": "raw",
          "supportedContainerTypes": [
            "page",
            "blogpost",
            "comment"
          ],
          "supportedChildTypes": [
            "attachment",
            "comment"
          ],
          "indexing": {
            "enabled": true
          }
        },
        "name": {
          "value": "ZenUML Sequence Diagram Lite"
        },
        "key": "zenuml-content-sequence"
      },
      {
        "uiSupport": {
          "contentViewComponent": {
            "moduleKey": "zenuml-content-graph-viewer"
          },
          "icons": {
            "item": {
              "width": 16,
              "height": 16,
              "url": "image/zenuml_logo.png"
            }
          }
        },
        "apiSupport": {
          "bodyType": "raw",
          "supportedContainerTypes": [
            "page",
            "blogpost",
            "comment"
          ],
          "supportedChildTypes": [
            "attachment",
            "comment"
          ],
          "indexing": {
            "enabled": true
          }
        },
        "name": {
          "value": "ZenUML Graph Lite"
        },
        "key": "zenuml-content-graph"
      }
    ],
    "generalPages": [
      {
        "key": "zenuml-content-sequence-viewer-dialog",
        "location": "none",
        "url": "/sequence-viewer-dialog.html?version=2021.11&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence",
        "name": {
          "value": "ZenUML Sequence Viewer Dialog"
        }
      },
      {
        "key": "zenuml-content-sequence-viewer",
        "location": "none",
        "url": "/sequence-viewer.html?version=2021.11&rendered.for=custom-content-native&spaceKey={space.key}&content.id={content.id}&content.version={content.version}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence",
        "name": {
          "value": "ZenUML Sequence Viewer"
        }
      },
      {
        "key": "zenuml-content-sequence-editor-dialog",
        "location": "none",
        "url": "/sequence-editor-dialog.html?version=2021.11&spaceKey={space.key}&content.id={content.id}&content.version={content.version}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-sequence",
        "name": {
          "value": "ZenUML Sequence Editor"
        }
      },
      {
        "key": "zenuml-content-graph-viewer",
        "location": "none",
        "url": "/drawio/viewer.html?version=2021.11&rendered.for=custom-content-native&spaceKey={space.key}&content.id={content.id}&content.version={content.version}&addonKey=com.zenuml.confluence-addon-lite&contentKey=zenuml-content-graph",
        "name": {
          "value": "ZenUML Graph Viewer"
        }
      }
    ]
  },
  "apiMigrations": {
    "gdpr": true
  },
  "vendor": {
    "name": "ZenUML",
    "url": "https://www.zenuml.com/"
  }
}